{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Day Counter",
  "description": "Mod my bender",
  "authors": [
    "Ender"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.theendercore.DayCounter"
      }
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric": "*",
    "minecraft": "1.19.3",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.9.4+kotlin.1.8.21"
  },
  "suggests": {
    "another-mod": "*"
  }
}
